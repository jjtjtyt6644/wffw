	{1, "Geppo"},
	{2, "Soru"},
	{3, "Buso"},
	{4, "Ken"}
}

for _, haki in ipairs(hakis) do
	hakiDropdown:Add(haki[1], haki[2], function(_, Title)
		_G.SelectedHaki = Title
		print("Selected Haki: " .. Title)
	end)
